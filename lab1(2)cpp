 int main() 
 {
  //Begin 19. Дано число A. Обчислити A8, 
  //використовуючи допоміжну змінну і три операції множення.
  //Для цього послідовно знайдіть A2, A4, A8.
  //Вивести всі знайдені степені числа A.
  // декларації 
 float A, A2, A4, A8;// Оголошення змінної 
 //введення
 cout << "Введіть невідому : ";
 //обчислення
 A2 = A * A;
 A4 = A2 * A2;
 A8 = A4 * A4;
 //виведення
 cout << "A * A * A4 * A8 = " << A;
 
  
    return 0;
}
